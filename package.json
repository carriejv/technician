{
	"name": "technician",
	"version": "2.3.0",
	"description": "Access, manage, cache, and export all your config from a single service. Supports Typescript w/ typed config value output.",
	"keywords": [
		"config",
		"configuration",
		"config file",
		"json",
		"secret",
		"environment",
		"parser",
		"reader",
		"typescript",
		"types",
		"technician"
	],
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"start": "node index.js",
		"test": "npm run lint && npm run testOnly",
		"testOnly": "nyc --reporter=html --reporter=text mocha -r ts-node/register ./test/**/*.spec.ts",
		"lint": "eslint ./src --ext .ts",
		"build": "tsc --project ./tsconfig.json",
		"docs": "typedoc --out docs src && echo \"Docs generated at ./docs/index.html\"",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/carriejv/technician.git"
	},
	"bugs": {
		"url": "https://github.com/carriejv/technician/issues"
	},
	"homepage": "https://github.com/carriejv/technician",
	"author": "Carrie Vrtis <carrie@carriejv.com> (https://www.carriejv.com)",
	"contributors": [
		"Carrie Vrtis <carrie@carriejv.com> (https://www.carriejv.com)"
	],
	"license": "Apache-2.0",
	"private": false,
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.37.0",
		"@types/chai": "^5.2.2",
		"@types/chai-spies": "^1.0.6",
		"@types/mocha": "^10.0.10",
		"@types/mockery": "^1.4.33",
		"@types/node": "^24.6.2",
		"@types/sinon": "^17.0.4",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"chai": "^6.2.0",
		"chai-spies": "^1.1.0",
		"eslint": "^9.37.0",
		"mocha": "^11.7.4",
		"mockery": "^2.1.0",
		"nyc": "^17.1.0",
		"ts-node": "^10.9.2",
		"typedoc": "^0.28.13",
		"typedoc-dark-theme": "0.0.11",
		"typescript": "^5.9.3"
	}
}
